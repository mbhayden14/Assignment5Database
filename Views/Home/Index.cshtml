@{
    ViewData["Title"] = "Home Page";
}


@model ProjectListViewModel


<!--Pull in information from partial view-->
@foreach (var x in Model.Projects)
{
    <partial name="BookSummary" model="x" />
}


<!--Add page buttons to the bottom of the page-->
<div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true" page-class="btn"
     page-class-normal="btn-outline-dark" page-class-selected="btn-primary" page-url-category="@Model.Category"
     class="btn-group pull-right m-1"></div>